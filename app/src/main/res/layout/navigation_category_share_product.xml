<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:dividerPadding="1dp">
    <RelativeLayout
        android:id="@+id/rl_nav_category_share_incomplete"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center">
        <TextView
            android:text="나눔 미완료"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#000000"
            android:fontFamily="@font/seoulnamsanm"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_nav_category_share_complete"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center">
        <TextView
                android:fontFamily="@font/seoulnamsanm"
            android:text="나눔 완료"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:textSize="14dp" android:textColor="#000000"
            />
    </RelativeLayout>
</LinearLayout>